package co.in.pp;

import java.util.*;

public class CityDTO 
{

	private Integer cityid;
	
	private String cityname;
	
	private CountryDTO countryParent;
	
	private StateDTO cityParent;

	
	
	public Integer getCityid() {
		return cityid;
	}

	public void setCityid(Integer cityid) {
		this.cityid = cityid;
	}

	public String getCityname() {
		return cityname;
	}

	public void setCityname(String cityname) {
		this.cityname = cityname;
	}

	public CountryDTO getCountryParent() {
		return countryParent;
	}

	public void setCountryParent(CountryDTO countryParent) {
		this.countryParent = countryParent;
	}

	public StateDTO getCityParent() {
		return cityParent;
	}

	public void setCityParent(StateDTO cityParent) {
		this.cityParent = cityParent;
	}

	
	
	
	
}
