package co.in.pp;

public class GadgetDTO 
{

	private Integer gid, gyearofpurchase;
	
	private String gname, gbrand;
	
	private UserDTO UserParent;

	public GadgetDTO() {
		super();
		// TODO Auto-generated constructor stub
	}

	public GadgetDTO(Integer gid, Integer gyearofpurchase, String gname, String gbrand) {
		super();
		this.gid = gid;
		this.gyearofpurchase = gyearofpurchase;
		this.gname = gname;
		this.gbrand = gbrand;
	}
	
	
	
	
	
	
}
