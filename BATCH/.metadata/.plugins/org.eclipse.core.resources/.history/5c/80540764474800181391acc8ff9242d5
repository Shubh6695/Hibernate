package co.in.pp;

public class Test 
{

	
	public static Integer generateRandom(Integer min, Integer max)throws Exception
	{
		
		Integer r=(int) ((Math.random() * ((max-min)+ 1)) +max);
		
		
		return r;
	    
		
	}
	
	
	public static void main(String[] args) throws Exception
	{
		
		
		
		System.out.println("Random no is :- "+generateRandom(1, 100));
		
		
	}
	
	
	
}
