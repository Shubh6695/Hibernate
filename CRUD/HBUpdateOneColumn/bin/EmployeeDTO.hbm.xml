<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="co.in">


<class name="EmployeeDTO" table="EmployeeDetails">

<id name="id" column="EID">
	<generator class="assigned"></generator>

</id>



<property name="fname" column="FNAME" update="false"></property>

<property name="lname" column="LNAME" update="false"></property>

<property name="address" column="ADDRESS" ></property>

<property name="mobile" column="MOBILE" update="false"></property>


</class>


</hibernate-mapping>
