package co.in.pp;

import javax.persistence.*;


@Entity
@Table(name="familywife")

public class WifeDTO 
{

	@Id
	@Column(name="W_ID")
	private Integer wid;
	
	
	@Column(name="W_NAME")
	private String wname;
	
	
	@Column(name="W_GENDER")
	private String wgender;
	
	
	private HusbandDTO husbandParent;
	
	
}
