package co.in.pp;

import javax.persistence.*;


@Entity
@Table(name="familyhusband")

public class HusbandDTO 
{

	@Id
	@Column(name="H_ID")
	private Integer hid;
	
	@Column(name="H_NAME")
	private String hname;
	
	
	@Column(name="H_GENDER")
	private String hegnder;


	public Integer getHid() {
		return hid;
	}


	public void setHid(Integer hid) {
		this.hid = hid;
	}


	public String getHname() {
		return hname;
	}


	public void setHname(String hname) {
		this.hname = hname;
	}


	public String getHegnder() {
		return hegnder;
	}


	public void setHegnder(String hegnder) {
		this.hegnder = hegnder;
	}
	
	
	
	
	
	
	
}
